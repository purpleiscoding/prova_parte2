<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.femass.controller.Cadastro_ClienteController">
    <children>
        <Pane minHeight="356.0" minWidth="225.0" prefHeight="356.0" prefWidth="225.0">
            <children>
                <ListView fx:id="LstClientes" layoutX="7.0" layoutY="22.0" onKeyPressed="#LstClientes_KeyPressed" onMouseClicked="#LstClientes_MouseClicked" prefHeight="287.0" prefWidth="166.0" />
                <Button fx:id="BtnExcluir" layoutX="22.0" layoutY="320.0" mnemonicParsing="false" onAction="#BtnExcluir_Action" text="Excluir Cliente" />
                <Label layoutX="198.0" layoutY="34.0" text="Nome do Cliente" />
                <Label layoutX="198.0" layoutY="78.0" text="Data de Nascimento" />
                <Label layoutX="198.0" layoutY="117.0" text="Documento" />
                <Label layoutX="198.0" layoutY="174.0" prefHeight="17.0" prefWidth="108.0" text="E-mail de Cadastro" />
            </children>
        </Pane>
        <Pane minHeight="356.0" minWidth="316.0" prefHeight="365.0" prefWidth="316.0">
            <children>
                <Button fx:id="BtnGravar" disable="true" layoutX="143.0" layoutY="256.0" mnemonicParsing="false" onAction="#BtnGravar_Action" prefHeight="36.0" prefWidth="121.0" text="Cadastrar Cliente" />
                <TextField fx:id="NomeCliente" disable="true" layoutX="105.0" layoutY="30.0" prefHeight="25.0" prefWidth="196.0" />
                <TextField fx:id="NascimentoCliente" disable="true" layoutX="105.0" layoutY="75.0" prefHeight="25.0" prefWidth="196.0" promptText="DD/MM/AAAA" />
                <TextField fx:id="DocumentoCliente" disable="true" layoutX="105.0" layoutY="115.0" prefHeight="25.0" prefWidth="196.0" promptText="CPF OU RG (SEM PONTUAÇÃO)" />
                <Button fx:id="BtnCancelar" disable="true" layoutX="-9.0" layoutY="256.0" mnemonicParsing="false" onAction="#BtnCancelar_Action" prefHeight="36.0" prefWidth="121.0" text="Cancelar Cadastro" />
                <TextField fx:id="EmailCliente" disable="true" layoutX="105.0" layoutY="170.0" prefHeight="25.0" prefWidth="196.0" />
            </children>
        </Pane>
    </children>
</HBox>
