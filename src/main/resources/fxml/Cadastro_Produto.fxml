                <Label layoutX="206.0" layoutY="117.0" text="Quantidade" />
                <Label layoutX="199.0" layoutY="149.0" />
                <Label layoutX="207.0" layoutY="166.0" text="Fornecedor" />
                <Label layoutX="207.0" layoutY="199.0" text="Valor do Produto" />
            </children>
        </Pane>
        <Pane minHeight="356.0" minWidth="316.0" prefHeight="365.0" prefWidth="316.0">
            <children>
                <TextField fx:id="SKUProduto" disable="true" layoutX="90.0" layoutY="71.0" prefHeight="25.0" prefWidth="211.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
