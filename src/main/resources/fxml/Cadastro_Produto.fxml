                <TextField fx:id="QuantidadeProduto" disable="true" layoutX="90.0" layoutY="114.0" prefHeight="25.0" prefWidth="211.0" />
                <TextField fx:id="FornecedorProduto" disable="true" layoutX="90.0" layoutY="158.0" prefHeight="25.0" prefWidth="211.0" />
                <TextField fx:id="ValorProduto" disable="true" layoutX="90.0" layoutY="195.0" prefHeight="25.0" prefWidth="211.0" />
            </children>
        </Pane>
    </children>
</HBox>
